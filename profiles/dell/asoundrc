# defaults.namehint.extended on
# defaults.pcm.dmix.rate = 44100
# defaults.pcm.dmix.!rate 44100

pcm.asymed {
   type asym
   playback.pcm "dmix"
   capture.pcm "dsnoop"
}

pcm.cnv {
    type rate
    slave {
        pcm hw:0
        rate 48000
    }
}

pcm.bluetooth {
        type bluealsa
        interface "hci0"
        device "88:C6:26:9A:DE:12"
        profile "a2dp"
}

pcm.cnvb {
    type rate
    slave {
        pcm bluetooth
        rate 48000
    }
}

# pcm.!default pcm.asymed
# pcm.!default pcm.null
# pcm.!default pcm.cnv
pcm.!default {
    @func getenv
    vars [ ALSAPCM ]
    default "pcm.asymed"
    # default "pcm.cnvb"
}
