# defaults.namehint.extended on
# defaults.pcm.dmix.rate = 44100
# defaults.pcm.dmix.!rate 44100

pcm.asymed {
   type asym
   playback.pcm "dmix"
   capture.pcm "dsnoop"
}

pcm.cnv {
    type rate
    slave {
        pcm hw:0
        rate 48000
    }
}

# pcm.!default pcm.asymed
# pcm.!default pcm.null
# pcm.!default pcm.cnv
pcm.!default {
    @func getenv
    vars [ ALSAPCM ]
    default "pcm.asymed"
}
